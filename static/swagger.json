			"propertyRequestPostBody": {
				"type": "object",
				"properties": {
					"address": {
						"type": "string",
						"format": "string"
					},
					"city": {
						"type": "string",
						"format": "string"
					},
					"state": {
						"type": "string",
						"format": "string"
					},
					"zip": {
						"type": "string",
						"format": "string"
					}

				}
			},
			"propertiesSchema": {
				"type": "object",
				"properties": {
					"id": {
						"type": "number",
						"format": "string"
					},
					"address": {
						"type": "string",
						"format": "string"
					},
					"city": {
						"type": "string",
						"format": "string"
					},
					"state": {
						"type": "string",
						"format": "string"
					},
					"zip": {
						"type": "string",
						"format": "string"
					}
				}
			},

		},
		"securitySchemas": {
			"ApiKeyAuth": {
				"type": "apiKey",
				"in": "header",
				"name": "Api-Key"
			}
		}
	}
